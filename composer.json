{
  "name": "mice-tm/yii2-timeline-widget",
  "type": "yii2-extension",
  "description": "A widget to render simple timeline",
  "keywords": ["yii2", "extension", "widget", "timeline"],
  "license": "MIT",
  "authors": [
    {
      "name": "Iryna Mychkova",
      "email": "imychkova@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "micetm\\timeline\\": "src"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "^2.0.13"
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    },
    "process-timeout": 1800
  }
}
